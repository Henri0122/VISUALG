Algoritmo "Calculadora"

var valorum: real
    valordois: real
    total: real

Inicio

   escreva ("Digite um valor:")
   leia (valorum)

   escreva ("Digite outro valor:")
   leia (valordois)
   
   total <- valorum + valordois

   escreval ("A soma entre ", valorum, " e", valordois, " é igual á ", total)

fimalgoritmo

