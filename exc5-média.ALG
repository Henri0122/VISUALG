Algoritmo "Média"

var notaum: real
    notadois: real
    media: real

Inicio

   escreva ("1°nota:")
   leia (notaum)

   escreva ("2°nota:")
   leia (notadois)
   
   media <- (notaum + notadois) / 2

   escreval ("A média entre ", notaum, " e", notadois, " é igual á ", media)

fimalgoritmo

