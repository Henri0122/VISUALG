algoritmo "Delta"

var a: real
   b: real
   c: real
   delta: real

inicio

   escreva (" Valor A:")
   leia (a)

   escreva ("Valor B:")
   leia (b)

   escreva ("Valor C:")
   leia (c)

   delta <- b^2 - 4 * a * c
   
   escreval ("O valor de delta é: ", delta)





fimalgoritmo