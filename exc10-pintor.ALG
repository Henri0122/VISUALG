algoritmo "Pintor"

var alt: real
    larg : real
    lata: real
    area: real
    
inicio

escreva ("Altura da parade:")
 leia (alt)
 
escreva ("Largura da parede:")
 leia (larg)
 
 area <- alt * larg
 
 lata <- area/2
 
 escreval ("A área da parede é de", area,"m², você irá precisar de ", lata," litros de tinta para pintá-la")



fimalgoritmo