Algoritmo "Boletim"
//Seçâo de Declaraçôes

var farenheit, celsius: real


Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   escreva("Digite a temperatura em farenheit: ")
   leia(farenheit)

   celsius <- 5* (farenheit-32) /9
   
   escreval (" A temperatura em graus celsius é de: ",  celsius)

Fimalgoritmo